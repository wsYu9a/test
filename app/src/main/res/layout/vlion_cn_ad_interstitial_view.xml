<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <cn.vlion.ad.inland.ad.base.VlionBaseAdView
        android:layout_gravity="center"
        android:id="@+id/vb_frameLayout"
        android:layout_width="match_parent"
        android:layout_height="match_parent">
        <FrameLayout
            android:layout_gravity="center"
            android:id="@+id/vlion_ad_inter_view_container"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"/>
    </cn.vlion.ad.inland.ad.base.VlionBaseAdView>
    <cn.vlion.ad.inland.ad.view.download.VlionDownLoadSecondConfirmView
        android:layout_gravity="center"
        android:orientation="vertical"
        android:id="@+id/vlionDownLoadSecondConfirmView"
        android:visibility="gone"
        android:layout_width="match_parent"
        android:layout_height="match_parent"/>
</FrameLayout>
