<?xml version="1.0" encoding="utf-8"?>
<FrameLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_gravity="center"
    android:layout_width="128dp"
    android:layout_height="128dp">
    <View
        android:layout_gravity="center"
        android:id="@+id/ksad_feed_shake_bg"
        android:background="@drawable/ksad_feed_shake_bg"
        android:visibility="invisible"
        android:layout_width="100dp"
        android:layout_height="100dp"/>
    <ImageView
        android:layout_gravity="center"
        android:id="@+id/ksad_feed_shake_icon"
        android:visibility="invisible"
        android:layout_width="55.56dp"
        android:layout_height="55.56dp"
        android:src="@drawable/ksad_ic_shake_hand"/>
</FrameLayout>
