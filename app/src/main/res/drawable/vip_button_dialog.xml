<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_pressed="true">
        <shape android:shape="rectangle">
            <corners android:radius="100dp"/>
            <gradient
                android:startColor="#99f3d2a2"
                android:endColor="#99a07141"
                android:angle="0"
                android:type="linear"/>
        </shape>
    </item>
    <item android:state_focused="true">
        <shape android:shape="rectangle">
            <corners android:radius="100dp"/>
            <gradient
                android:startColor="#99f3d2a2"
                android:endColor="#99a07141"
                android:angle="0"
                android:type="linear"/>
        </shape>
    </item>
    <item>
        <shape android:shape="rectangle">
            <corners android:radius="100dp"/>
            <gradient
                android:startColor="#f3d2a2"
                android:endColor="#a07141"
                android:angle="0"
                android:type="linear"/>
        </shape>
    </item>
</selector>
